{
    "name": "hexlet/code",
    "description": "this utility generates difference between two .json/.yaml files",
    "type": "library",
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^5.4",
        "lstrojny/functional-php": "^1.17"
    },
    "bin": [
        "bin/gendiff"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files": [
            "src/Parsers/JsonParser.php",
            "src/Differ/Generator.php",
            "src/Parsers/YamlParser.php",
            "src/Differ/DiffBuilder.php",
            "src/Differ/Structures/DiffTree.php",
            "src/Formatters/PlainFormatter.php",
            "src/Formatters/StylishFormatter.php",
            "src/Formatters/JsonFormatter.php"
        ]
        
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Viachaslau Viarbitski",
            "email": "viachaslauviarbitski@gmail.com"
        }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "allow-plugins": {
            
        }
    }
}
